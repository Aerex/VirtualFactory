        <TextView android:id="@string/login_string" android:textSize="20sp" android:layout_height="wrap_content" android:gravity="center" android:text="@string/login_string" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_alignTop="@+id/relativeLayout2" android:layout_centerHorizontal="true"></TextView>
        <TableRow android:layout_height="wrap_content" android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_below="@string/login_string" android:layout_toRightOf="@+id/relativeLayout2" android:layout_marginTop="28dp" android:paddingBottom="25px">
