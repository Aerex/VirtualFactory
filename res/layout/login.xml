        <RelativeLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/relativeLayout2" android:layout_below="@+id/imageView1" android:layout_marginTop="96dp" android:layout_toRightOf="@+id/imageView1">
